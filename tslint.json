{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "array-type": false,
        "arrow-parens": false,
        "arrow-return-shorthand": true,
        "align": [
            true,
            "parameters",
            "statements",
            "members"
        ],
        "deprecation": {
            "severity": "warn"
        },
        "eofline": true,
        "import-spacing": true,
        "interface-name": [
            false,
            "always-prefix"
        ],
        "max-classes-per-file": false,
        "max-line-length": [
            true,
            120
        ],
        "member-access": false,
        "member-ordering": [
            true,
            {
            "order": [
                "static-field",
                "instance-field",
                "static-method",
                "instance-method"
            ]
            }
        ],
        "no-consecutive-blank-lines": false,
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "no-empty": false,
        "no-inferrable-types": [
            false,
            "ignore-params"
        ],
        "no-non-null-assertion": true,
        "no-redundant-jsdoc": true,
        "no-switch-case-fall-through": true,
        "no-use-before-declare": false,
        "no-var-requires": false,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "trailing-comma": [
            true,
            {
            "multiline": {
                "arrays": "never",
                "exports": "never",
                "functions": "never",
                "imports": "never",
                "objects": "never",
                "typeLiterals": "never"
            },
            "singleline": {
                "arrays": "never",
                "exports": "never",
                "functions": "never",
                "imports": "never",
                "objects": "never",
                "typeLiterals": "never"
            },
            "esSpecCompliant": true
            }
        ],
        "only-arrow-functions": false,
        "no-forward-ref": false,
        "no-irregular-whitespace": true,
        "no-trailing-whitespace": true,
        "no-string-literal": false,
        "space-before-function-paren": false,
        "space-within-parens": [
            true,
            0
        ],
        "semicolon": [
            true,
            "always"
        ],
        "variable-name": [
            true,
            "allow-pascal-case",
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ],
        "no-namespace": false
        },
        "rulesDirectory": [],
        "linterOptions": {
        "exclude": [
            "node_modules"
        ]
    }
}